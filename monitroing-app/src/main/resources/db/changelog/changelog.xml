<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd"
        logicalFilePath="changelog.xml">

    <preConditions>
        <dbms type="postgresql"/>
    </preConditions>

    <include file="V1_create-schema.xml" relativeToChangelogFile="true"/>
    <include file="V2_create-sequence.xml" relativeToChangelogFile="true"/>
    <include file="V3_create-tables.xml" relativeToChangelogFile="true"/>
    <include file="V4_insert-data.xml" relativeToChangelogFile="true"/>
</databaseChangeLog>